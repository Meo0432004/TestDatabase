<Window x:Class="TestDatabaseGithub.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TestDatabaseGithub"
        xmlns:vm="clr-namespace:TestDatabaseGithub.ViewModel"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.DataContext>
        <vm:RoleNameViewModel></vm:RoleNameViewModel>
    </Window.DataContext>
    <Grid>
        <ListView Margin="38,164,66,72" ItemsSource="{Binding Roles}">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="ID" DisplayMemberBinding="{Binding Id, Mode=TwoWay}" Width="100"></GridViewColumn>
                    <GridViewColumn Header="RoleName" DisplayMemberBinding="{Binding Name, Mode=TwoWay}" Width="100"></GridViewColumn>
                    
                </GridView>
            </ListView.View>
        </ListView>
    </Grid>
</Window>
